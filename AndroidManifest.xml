<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.trackapp2">

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login_activity" />
        <activity android:name=".Create_accountActivity" />
        <activity android:name=".Welcome2" />
        <activity android:name=".ActivityY" />
        <activity android:name=".Alarm" />

        <receiver android:name=".AlarmReceiver" />

        <activity android:name=".AlarmReceiverActivity" />
        <activity android:name=".Reminder" />
        <activity android:name=".TrackAlarm" />
    </application>

</manifest>